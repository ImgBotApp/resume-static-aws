{
  "name": "resume-static-aws",
  "version": "0.0.0",
  "description": "A resume website",
  "main": "index.js",
  "scripts": {
    "flow": "flow",
    "test:integration": "nightwatch",
    "test": "mocha --watch \"spec/unit/**/*.js\"",
    "start": "rm -rf dist && NODE_ENV=development webpack-dev-server -d --config tools/webpack.config.dev.babel.js --host 0.0.0.0 --hot --inline --content-base dist/ --color --bail",
    "start:prod": "rm -rf dist && NODE_ENV=production webpack-dev-server -p --config tools/webpack.config.prod.babel.js --display-chunks --display-reasons --display-error-details --hot --inline --content-base dist/ --color --bail",
    "build:prod": "rm -rf dist && NODE_ENV=production webpack -p --config tools/webpack.config.prod.babel.js --progress --display-chunks --display-reasons --display-error-details --bail",
    "build:dev": "rm -rf dist && NODE_ENV=development webpack -d --config tools/webpack.config.dev.babel.js --progress --display-chunks --display-reasons --display-error-details --bail",
    "deploy": "rm -rf dist && NODE_ENV=production webpack -p --config tools/webpack.config.deploy.babel.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Travmatth/resume-static-aws.git"
  },
  "author": "Travis Matthews",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Travmatth/resume-static-aws/issues"
  },
  "homepage": "https://github.com/Travmatth/resume-static-aws#readme",
  "dependencies": {
    "babel-polyfill": "^6.16.0",
    "babel-runtime": "^6.18.0",
    "virtual-dom": "^2.1.1"
  },
  "devDependencies": {
    "autoprefixer": "^6.5.3",
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.8",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.18.0",
    "babel-plugin-transform-flow-strip-types": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-0": "^6.16.0",
    "chromedriver": "^2.25.1",
    "css-loader": "^0.26.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "flow-bin": "^0.35.0",
    "html-webpack-plugin": "^2.24.1",
    "json-loader": "^0.5.4",
    "mocha": "^3.1.2",
    "nighthawk": "^1.4.0",
    "nightwatch": "^0.9.9",
    "node-sass": "^3.13.0",
    "postcss-loader": "^1.1.1",
    "pug": "^2.0.0-beta6",
    "pug-loader": "^2.3.0",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2",
    "webpack-merge": "^0.17.0",
    "webpack-s3-plugin": "^0.9.2",
    "webpack-stats-plugin": "^0.1.4",
    "webpack-validator": "^2.2.9"
  }
}
