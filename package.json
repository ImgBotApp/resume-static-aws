{
  "name": "resume-static-aws",
  "version": "0.0.0",
  "description": "A resume website",
  "main": "index.js",
  "scripts": {
    "flow": "flow",
    "flow:status": "flow status",
    "DELETE": "mocha -r babel-polyfill --compilers js:babel-core/register --watch \"src/**/*.spec.js\"",
    "test": "ava --verbose **/*.spec.js",
    "start": "rm -rf dist && NODE_ENV=development webpack-dev-server -d --config tools/webpack.config.dev.babel.js --host 0.0.0.0 --hot --inline --content-base dist/ --color --bail",
    "stats": "webpack --progress --display-reasons --display-error-details ",
    "start:prod": "rm -rf dist && NODE_ENV=production webpack-dev-server -p --config tools/webpack.config.prod.babel.js --hot --inline --content-base dist/ --color --bail",
    "build:prod": "rm -rf dist && NODE_ENV=production webpack -p --config tools/webpack.config.prod.babel.js --bail",
    "build:dev": "rm -rf dist && NODE_ENV=development webpack -d --config tools/webpack.config.dev.babel.js --bail",
    "deploy": "rm -rf dist && NODE_ENV=production webpack -p --config tools/webpack.config.deploy.babel.js",
    "build:parser": "pegjs src/calculator/arithmetic.pegjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Travmatth/resume-static-aws.git"
  },
  "author": "Travis Matthews",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Travmatth/resume-static-aws/issues"
  },
  "homepage": "https://github.com/Travmatth/resume-static-aws#readme",
  "ava": {
    "require": [
      "babel-core/register",
      "babel-polyfill"
    ],
    "babel": {
      "babelrc": true
    }
  },
  "dependencies": {
    "babel-polyfill": "^6.16.0",
    "babel-runtime": "^6.18.0",
    "extendable-error-class": "^0.1.1",
    "virtual-dom": "^2.1.1"
  },
  "devDependencies": {
    "autoprefixer": "^6.5.3",
    "ava": "^0.17.0",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.8",
    "babel-plugin-transform-async-to-generator": "^6.16.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.18.0",
    "babel-plugin-transform-flow-strip-types": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.18.0",
    "css-loader": "^0.26.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "fetch-mock": "^5.8.0",
    "flow-bin": "^0.36.0",
    "flow-status-webpack-plugin": "^0.1.7",
    "html-webpack-plugin": "^2.24.1",
    "jquery": "^3.1.1",
    "jsdom": "^9.9.1",
    "jsdom-global": "^2.1.1",
    "json-loader": "^0.5.4",
    "node-sass": "^4.0.0",
    "pegjs": "^0.10.0",
    "pegjs-loader": "^0.5.0",
    "postcss-loader": "1.2.1",
    "pug": "^2.0.0-beta6",
    "pug-loader": "^2.3.0",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "svgo": "^0.7.1",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2",
    "webpack-merge": "^0.17.0",
    "webpack-s3-plugin": "^0.9.2",
    "webpack-stats-plugin": "^0.1.4",
    "webpack-validator": "^2.2.9"
  }
}
