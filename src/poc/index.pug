extends ../common/layout

block heading
	if (htmlWebpackPlugin)
		script(src=htmlWebpackPlugin.files.js[0])
		link(href=`${htmlWebpackPlugin.files.css[0]}` rel="stylesheet" type="text/css")

block content
	h1 #{JSON.stringify(htmlWebpackPlugin)}
	div.view
	div.center-panel
		input#search-text
		button#search-btn
		button#random-btn
	div.result-container
		div.heading
			h1
			h2
		p#anchor Some Text
