[ignore]
.*/node_modules/ava/.*.flow

[include]

[libs]
flow-typed

[options]
traces=10
suppress_comment=\\(.\\|\n\\)*\\$FlowIgnore
module.name_mapper='^common\/\(.*\)$' -> '<PROJECT_ROOT>/src/common/\1'
module.name_mapper='^tests\/\(.*\)$' -> '<PROJECT_ROOT>/tests/\1'
module.name_mapper='^\.\./index\.pug$' -> '<PROJECT_ROOT>/flow-typed/pug-stub.js'
